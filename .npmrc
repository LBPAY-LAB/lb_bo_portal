# Dependency Resolution
legacy-peer-deps=false
strict-peer-dependencies=false

# Performance
prefer-offline=true
fetch-retries=3
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Security
audit-level=moderate

# Logging
loglevel=warn

# Save exact versions
save-exact=true
save-prefix=""

# Engine Strict (enforce Node.js version)
engine-strict=true

# Progress
progress=true
